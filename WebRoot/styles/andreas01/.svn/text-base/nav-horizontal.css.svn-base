/* A CSS Framework by Mike Stenhouse of Content with Style */ /* NAV */
div#nav {
	width:100%;
	font-size: 0.9em;
	text-align: left;
}
div#topMenu {
	height:41px;
	background: url(img/left_top_meun.png) left top no-repeat;
	border-bottom: 1px solid #0476C1;
	font-size: 13px;
	padding-top:3px;
	padding-left:8px;
	line-height:18px;
	text-align: left;
	color:#FFFFFF;
	overflow: hidden;
	}
div#topMenu a {
	color:#f6ff00;
	text-decoration:underline;
	margin-right:3px;
	}
div#topMenu a:hover {
	color:#f6ff00;
	text-decoration: none;
	background:none;
	}
div#topMenu span{
	font-size: 12px;
	color: #012259;
	margin-right:3px;
	}	
div#nav ul {
	list-style: none;
	width: 100%;
	margin: 0;
	padding: 0;
}

div#nav li {
	margin: 0px 0px 0px 0px
}

div#nav li.last {
	margin-bottom: 0;
}

div#nav li a {

	height: 26px;
	text-decoration: none;
	color: #28588D;/*#505050;*/
	display: block;
	/*margin-bottom: 5px;*/
}

div#nav .pad {
	display: none;
}

div#local li a:hover,div#nav li a:hover,div#nav li a.current {
	color: #286ea0;
	border-left: 4px;
}

ul#local li {
	list-style: none;
	margin-left: 10px;
}

/* END NAV */ /* DROP DOWN MENUS */
ul#primary-nav,ul#primary-nav ul {
	margin: 0;
	padding: 0;
	font-size: 100%;
}

ul#primary-nav li {
	position: relative;
	list-style: none;
}

ul#primary-nav li a {
	display: block;
	text-decoration: none;
	color: #09306d;
	font-size: 14px;
	padding-left: 28px;
	line-height:24px;
	height: 26px;
	
}

/* Fix IE. Hide from IE Mac \*/
* html ul#primary-nav li {
	float: left;
	height: 1%;
}

* html ul#primary-nav li a {
	height: 1%;
}

* html ul#primary-nav ul {
	width: 151px;
}

* html ul#primary-nav ul li {
	width: 151px;
}

/* End */
ul#primary-nav ul {
	left: 151px; /* Set 1px less than menu width */
	top: 0;
	border: 0px solid black;
	white-space: nowrap;
}

ul#primary-nav li ul li.menu {
	position:relative;
	color: #4B4B4B;
	/*BACKGROUND: #97D4FF;/*#cae5ff;*/	
	Background:url("img/submenu_bg.png") repeat-x;
	OVERFLOW: hidden;
	white-space: nowrap;
	height: 25px;
	font-weight: normal;
}

ul#primary-nav li ul li a {
	font-size: 13px;
	font-weight: normal;
	border:0px;
	Background:url("img/icon2.gif") no-repeat 9px 8px;
	padding-left:20px;
}

ul#primary-nav li ul li a:hover {
	color: #07006a;
	OVERFLOW: hidden;
	BACKGROUND: url("img/submenu_hover.png") left center repeat-x;
	white-space: nowrap;
	border-top:0px;
	border-bottom:0px;
	font-size: 13px;
	font-weight:bold;
}

/* Sub Menu Styles */
ul#primary-nav li.menubar {
	BACKGROUND:url("img/menu_bg.png") repeat-x;
	margin-bottom: 1px;
	font-weight:bold;
}
ul#primary-nav li.menubar span{
	BACKGROUND:url("../../img/arrow2.gif") no-repeat;
	height: 11px;
    position: absolute;
    right: 10px;
    top: 8px;
    width: 15px;
}
ul#primary-nav li.over span{
	BACKGROUND:url("../../img/arrow1.gif") no-repeat;
	height: 11px;
    position: absolute;
    right: 10px;
    top: 8px;
    width: 15px;
}
.MenuLi1 em, .MenuLi2 em, .MenuLi3 em, .MenuLi4 em, .MenuLi5 em, .MenuLi6 em, .MenuLi7 em,
 .MenuLi8 em, .MenuLi9 em, .MenuLi10 em, .MenuLi11 em, .MenuLi12 em, .MenuLi13 em{
	background-image:url(img/menuIcon.png);
	height:22px;
	position: absolute;
    left: 3px;
    top: 3px;
    width: 24px;
    display:block;
    background-repeat:no-repeat;
}
.MenuLi1 em{
	background-position:0px 0px;
}
.MenuLi2 em{
	background-position:0px -24px;
}
.MenuLi3 em{
	background-position:0px -45px;
}
.MenuLi4 em{
	background-position:0px -66px;
}
.MenuLi5 em{
	background-position:0px -86px;
}
.MenuLi6 em{
	background-position:0px -108px;
}
.MenuLi7 em{
	background-position:0px -130px;
}
.MenuLi10 em{
	background-position:0px -151px;
}
.MenuLi11 em{
	background-position:0px -175px;
}
.MenuLi12 em{
	background-position:0px -196px;
}
.MenuLi13 em{
	background-position:0px -218px;
}
.MenuLi8 em{
	background-position:0px 0px;
}
.MenuLi9 em{
	background-position:0px 0px;
}

ul#primary-nav li.menu {
	/*background:url("img/submenu_bg.png") repeat-x;*/
	
}

ul#primary-nav li.over {
	BACKGROUND: url("img/menu_hover.png") repeat-x;
	margin-bottom:1px;
	font-weight:bold;
	color:#000;
}

ul#primary-nav li a:hover {
	BACKGROUND:  url("img/menu_hover.png") left top repeat-x;
	border:0px solid #ece9cf;
	height:26px;
	font-weight:bold;
	color:#000;
	/*#f2f9fd;*/
}

/* END DROP DOWN MENUS */